    </BuildMacro>
    <BuildMacro Include="SQLITE_WIN32_LIB_PATH">
        <Value>$(SQLITE_WIN32_LIB_PATH)</Value>
    </BuildMacro>
    <BuildMacro Include="SQLITE_WIN64_LIB_PATH">
        <Value>$(SQLITE_WIN64_LIB_PATH)</Value>
    </BuildMacro>
        
